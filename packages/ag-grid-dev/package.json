{
  "name": "ag-grid-dev",
  "version": "21.2.1",
  "description": "ag-Grid Modularisation Tests",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:modules && npm run test:bundle-size",
    "test:modules": "cd tests && ./runTests.sh",
    "test:bundle-size": "cd bundle-size-tests && ./compareBundleSizes.sh",
    "build:bundle-size": "cd bundle-size-tests && ./downloadCurrentBundles.sh",
    "build:test-input": "cd test-config && node buildTests.js",
    "build:bundles": "cd test-input && ./webpack.sh",
    "build:moduleParser": "tsc -p tsconfig-moduleParser.json",
    "build": "npm run build:moduleParser && npm run build:test-input && npm run build:bundles && npm run build:bundle-size"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanlandsman/ag-grid-modules-test.git"
  },
  "author": "Sean Landsman <sean@thelandsmans.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seanlandsman/ag-grid-modules-test/issues"
  },
  "homepage": "https://github.com/seanlandsman/ag-grid-modules-test#readme",
  "devDependencies": {
    "@types/node": "^11.12.2",
    "ag-grid-community": "^21.2.0",
    "ag-grid-enterprise": "^21.2.0",
    "autoprefixer": "^9.6.0",
    "awesome-typescript-loader": "5.2.1",
    "css-loader": "1.0.0",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "loader-utils": "1.1.0",
    "node-sass": "^4.6.0",
    "postcss-loader": "3.0.0",
    "postcss-scss": "2.0.0",
    "sass-loader": "^6.0.6",
    "source-map-loader": "0.2.4",
    "style-loader": "0.23.1",
    "ts-loader": "5.2.2",
    "typescript": "^3.1.3",
    "url-loader": "1.1.2",
    "webpack": "4.22.0",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.4",
    "webpack-merge": "^4.2.1"
  }
}
